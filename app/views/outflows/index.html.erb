<p id="notice"><%= notice %></p>

<h1>Outflows</h1>

<table>
  <thead>
    <tr>
      <th>Fecha</th>
      <th>Concepto</th>
      <th>Nit</th>
      <th>Entidad</th>
      <th>Numero factura</th>
      <th>Valor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @outflows.each do |outflow| %>
      <tr>
        <td><%= outflow.fecha %></td>
        <td><%= outflow.concepto %></td>
        <td><%= outflow.nit %></td>
        <td><%= outflow.entidad %></td>
        <td><%= outflow.numero_factura %></td>
        <td><%= outflow.valor %></td>
        <td><%= link_to 'Show', outflow %></td>
        <td><%= link_to 'Edit', edit_outflow_path(outflow) %></td>
        <td><%= link_to 'Destroy', outflow, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Outflow', new_outflow_path %>
